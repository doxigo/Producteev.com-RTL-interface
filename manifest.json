{
  "manifest_version": 2,

  "name": "Producteev RTL",
  "description": "Makes Producteev.com workspace interface RTL, suited for Persian language, Font used is BYekan.",
  "version": "1.3.1.1",
  "homepage_url": "http://netraz.com",
  "author": "Sohail Lajevardi doxgio@gmail.com",
  "minimum_chrome_version": "16.0",

  "permissions": [
    "https://producteev.com/workspace/*",
    "https://www.producteev.com/workspace/*"
  ],
    "content_scripts": [
    {
      "matches": ["https://producteev.com/workspace/*", "https://www.producteev.com/workspace/*"],
      "css": ["producteev-rtl.css"]
    }
  ],

  "browser_action": {
    "default_icon": "icon-128.png",
    "default_popup": "popup.html",
    "default_title": "Producteev RTL Interface"
  },

  "icons": { "16": "icon-16.png",
           "32": "icon-32.png",
          "128": "icon-128.png" }

}
